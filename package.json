{
  "name": "backend",
  "version": "1.0.0",
  "description": "backend for personal website",
  "main": "app.js",
  "bin": {
    "generate": "src/util/populate_fixtures.js"
  },
  "scripts": {
    "knex": "knex",
    "migrate": "knex migrate:latest",
